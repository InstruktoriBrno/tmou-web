parameters:
    canonical:
         url: http://www.tmou.test
    keycloak:
        authServerUrl:
        realm:
        clientId:
        clientSecret:
        redirectUri:
        version: # at least 20.0.2
    mail:
        noReply:
        replyTo:
    sso:
        cookieName: TMOU_SSO
        jwtCookieName: TMOU_SSO_JWT
        cookieDomain: tmou.test
        expiration: '+2 days' # should be lower than session.expiration below
        jwt:
            algorithm: HS512
            secretKey: # secret key which is used to for encrypting and decrypting the JWT tokens
    mailgun:
        apiKey:
        apiBaseUrl:
        domain:
    cron:
        key:
    fio:
        apiURL: https://fioapi.fio.cz/v1/rest/periods/__TOKEN__/__FROM__/__TO__/transactions.json
        token:
        notificationEmail:

captcha:
    type: turnstile
    secretKey: secretKey
    siteKey: siteKey
    theme: light
    size: normal

application:
    errorPresenter: Error
    mapping: InstruktoriBrno\TMOU\Presenters\*Presenter

di:
	export:
		parameters: no
		tags: no

session:
    expiration: 14 days


