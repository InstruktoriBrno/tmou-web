latte:
    extensions:
        - Nextras\FormsRendering\LatteTags\Bs3\Bs3FormsExtension

extensions:
    console: Contributte\Console\DI\ConsoleExtension(%consoleMode%)
    captcha: DrabekDigital\Captcha\DI\CaptchaExtension

    # Common
    nettrine.migrations: Nettrine\Migrations\DI\MigrationsExtension

    # DBAL
    nettrine.dbal: Nettrine\DBAL\DI\DbalExtension

    # ORM
    nettrine.orm: Nettrine\ORM\DI\OrmExtension

nettrine.migrations:
    table: doctrine_migrations
    column: version
    directory: %appDir%/../App/Migrations
    namespace: InstruktoriBrno\TMOU\Migrations
    versionsOrganization: null # null, year, year_and_month

decorator:
    InstruktoriBrno\TMOU\Presenters\BasePresenter:
        setup:
            - setBuildTime(%buildTime%)
    InstruktoriBrno\TMOU\Presenters\Error4xxPresenter:
        setup:
            - setBuildTime(%buildTime%)
    InstruktoriBrno\TMOU\Presenters\CronPresenter:
        setup:
            - setApiKey(%cron.key%)
nettrine.dbal:
    types:
        uuid_binary: Ramsey\Uuid\Doctrine\UuidBinaryType
        organizator_role: InstruktoriBrno\TMOU\Model\DoctrineTypes\OrganizatorRoleDoctrineType
        game_status: InstruktoriBrno\TMOU\Model\DoctrineTypes\GameStatusType
        payment_status: InstruktoriBrno\TMOU\Model\DoctrineTypes\PaymentStatusType
    typesMapping:
        uuid_binary: binary
        organizator_role: organizator_role
        game_status: game_status
        payment_status: payment_status
    debug:
        panel: true
        sourcePaths: [%appDir%]
    connections:
        default:
            wrapperClass: InstruktoriBrno\TMOU\Utils\TunedConnection
            driver: pdo_mysql
            host: db
            user: tmou
            password: password
            dbname: tmou
#            persistent: false
            charset: utf8mb4

nettrine.orm:
    managers:
        default:
            connection: default
            mapping:
                App:
                    directories: [%appDir%/Model]
                    namespace: InstruktoriBrno\TMOU\Model
            defaultCache: Contributte\Psr6\CachePool(
                Nette\Caching\Cache(
                  Nette\Caching\Storages\FileStorage(%tempDir%/cache)
                  doctrine/dbal
                )
              )

console:
    name: TMOU web
    catchExceptions: true
    autoExit: true
    url: %canonical.url%
